{
  "name": "docute-demo-code",
  "description": "ðŸ”¥Live code block for better showcase for Docute",
  "version": "0.1.5",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "evillt/docute-demo-code"
  },
  "author": "evillt <ijoec123@gmail.com> (https://evila.me)",
  "main": "dist/index.min.js",
  "files": [
    "dist/*.js"
  ],
  "scripts": {
    "commit": "git-cz",
    "prepublishOnly": "npm run build && npm run build:min",
    "build": "bili --format umd",
    "build:min": "bili --format umd-min"
  },
  "dependencies": {},
  "devDependencies": {
    "bili": "^4.4.0",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "prettier": "^1.15.2",
    "rollup-plugin-typescript2": "^0.20.1",
    "typescript": "^3.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,js,json,md}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "keywords": [
    "vue",
    "docute",
    "demo",
    "document",
    "documentation",
    "plugin"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
